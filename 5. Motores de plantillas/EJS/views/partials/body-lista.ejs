<div class="center">
    <h1><%=mensaje%></h1>
    <div class="tabla">
        <% if(resultado=='Archivo productos no encontrado'){ %>
            <h3>No hay productos</h3>
        <% } else {%>
            <table>
                <tr>
                    <th>nombre</th>
                    <th>precio</th>
                    <th>imagen</th>
                </tr>
                <% resultado.forEach(function(val) { %>        
                    <tr>
                        <td><%= val.title %></td>
                        <td>$<%= val.price %></td>
                        <td><img src="<%= val.thumbnail %>" alt=""></td>
                    </tr>
                <% }); %> 
            </table>
        <% } %>
    </div>
    <hr>
    <a href="/" class="btn btn-primary">Agregar Producto</a>
</div>
